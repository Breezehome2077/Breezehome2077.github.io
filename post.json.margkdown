permalink: /tool/posts.json
---

[{% for post in site.posts %}
    {
        "post_title":    {{ post.title | jsonify }},
        "post_url":      {{ post.url | jsonify }},
        "publication_date":     {{ post.date | date:"%Y-%m-%d" | jsonify }},
        "post_categories":   {{ post.categories | jsonify }},
        "post_tags":     {{ post.tags | jsonify }}
    },
{% endfor %}]